  file = {{Lin1994vn.pdf:/Users/walton/Google Drive/Papers/1994/L/Lin1994vn.pdf:application/pdf}},
  journal = {Neuron},
  language = {eng},
  month = {sep},
  number = {3},
  pages = {507--523},
  pmid = {7917288},
  rating = {0},
  title = {{Ectopic and increased expression of Fasciclin II alters motoneuron growth cone guidance}},
  volume = {13},
  year = {1994},
  abstract = {We used the enhancer detection/GAL4 system in Drosophila to direct increased levels of Fasciclin II (Fas II) expression on motoneuron growth cones and axons and to direct ectopic Fas II expression on other cells they encounter. Four classes of abnormal phenotypes are observed: {bypass} phenotypes, in which axons fail to defasciculate at the choice point where they would normally enter their muscle target region and instead extend past their target; {detour} phenotypes, in which these bypass growth cones enter their muscle target region at a different location; {stall} phenotypes, in which axons that enter their muscle target region fail to defasciculate from one another to probe their muscle targets; and {misroute} phenotypes, in which growth cones are diverted onto abnormal pathways by contact with Fas II-positive cells. These phenotypes show that changes in the pattern and level of Fas II expression can alter growth cone guidance, apparently in part by modulating the ability of these growth cones to respond to other guidance cues.},
  bdsk-file-1 = {},
  bdsk-url-1 = {http://www.sciencedirect.com/science?_ob=ArticleURL&_udi=B6WSS-4C71KSX-R&_user=170364&_rdoc=1&_fmt=&_orig=search&_sort=d&view=c&_acct=C000013318&_version=1&_urlVersion=0&_userid=170364&md5=b25e82e481b697e359f9edfea123a32f},
